<table border="1">
  <tr>
		<th>ID</th>
    <th>Apellidos</th>
		<th>Nombre</th>
		<th>DNI</th>
		<th>Año</th>
		<th>Categoría</th>
		<th>Género</th>
		<th>Club</th>
		<th>Federado</th>
		<th>Talla</th>
		<th>Pagado</th>
  </tr>
  <% @swimmers.each do |s| %>
  <tr>
		<td><%= s.id %></td>
   	<td><%= s.secname %></td>
		<td><%= s.name %></td>
		<td><%= s.dni %></td>
		<td><%= s.year %></td>
		<td><%= s.cat %></td>
		<td><%= s.gen %></td>
		<td><%= s.club %></td>
		<td><%= s.fed %></td>
		<td><%= s.size %></td>
		<td><% if s.paid %>Si<% else %>No<% end %></td>		
  <% end %>
 </tr>
</table>