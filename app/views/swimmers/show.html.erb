			<div class="dato"><%= @swimmer.postal %></div>
			
			<label for="inscription_prov">Provincia
				<span class="small"></span>
			</label>
			<div class="dato"><%= @swimmer.prov %></div>
			
			<label for="inscription_dir">Dirección
				<span class="small"></span>
			</label>
			<div class="dato"><%= @swimmer.dir %></div>
			
			<label for="inscription_size">Talla Camiseta
